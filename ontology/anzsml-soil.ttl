<https://anzsoil.org/ont/soil>
        a                owl:Ontology ;
        owl:imports      <http://def.seegrid.csiro.au/isotc211/iso19156/2011/sampling.ttl> , <http://def.seegrid.csiro.au/isotc211/iso19115/2003/metadata.ttl> , <http://schemas.geosciml.org/rdf/types.ttl> ;
        owl:versionIRI   "https://soilmlie.org/def/soilml#/anzsmls/1.0.0" ;
        owl:versionInfo  "1.0.0" ;
        skos:definition  "The ANZSoilML_Soil Package contains classes representing notional soil units, soil profiles and soil layers whose complete and precise extent is inferred to exist. Explicit spatial properties are available through association with a SpatialEntity via the AbstractSoil generalization. \r\n\r\nSoils includes both formal units (i.e. formally adopted and named in the official lexicon) and informal units (i.e. named but not promoted to the lexicon) and unnamed units (i.e. recognisable and described and delineable in the field but not otherwise formalised)."@en .

anzsmls:SoilFeature  a         owl:Class ;
        rdfs:subClassOf        anzsmls:AbstractSoil ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:Colour ;
                                 owl:onProperty     anzsmls:soilColour
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  swe:Quantity ;
                                 owl:onProperty     anzsmls:horizonRDepth
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  skos:Concept ;
                                 owl:onProperty     anzsmls:carbonateEffervescence
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  sf:SF_SamplingFeature ;
                                 owl:onProperty     anzsmls:SoilFeature.samplingFeature
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  skos:Concept ;
                                 owl:onProperty     anzsmls:SoilFeature.relatedObservation
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  skos:Concept ;
                                 owl:onProperty     anzsmls:relatedSample
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  skos:Concept ;
                                 owl:onProperty     anzsmls:drainage
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:SoilClassification ;
                                 owl:onProperty     anzsmls:classifier
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  skos:Concept ;
                                 owl:onProperty     anzsmls:landformPosition
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:SoilMaterial ;
                                 owl:onProperty     anzsmls:composition
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:SoilClassification ;
                                 owl:onProperty     anzsmls:classifier
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  skos:Concept ;
                                 owl:onProperty     anzsmls:purpose
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:Void ;
                                 owl:onProperty     anzsmls:poreSpace
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:SoilFeature ;
                                 owl:onProperty     anzsmls:relatedSoilFeature
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  skos:Concept ;
                                 owl:onProperty     anzsmls:landformPosition
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  skos:Concept ;
                                 owl:onProperty     anzsmls:purpose
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:SoilMaterial ;
                                 owl:onProperty     anzsmls:composition
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:Void ;
                                 owl:onProperty     anzsmls:poreSpace
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:Roots ;
                                 owl:onProperty     anzsmls:rootComponent
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  skos:Concept ;
                                 owl:onProperty     anzsmls:SoilFeature.observationMethod
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:SoilFeature ;
                                 owl:onProperty     anzsmls:relatedSoilFeature
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:Colour ;
                                 owl:onProperty     anzsmls:soilColour
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:Mottle ;
                                 owl:onProperty     anzsmls:colourPattern
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:Substrate ;
                                 owl:onProperty     anzsmls:associatedSubstrate
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:PhysicalPropertyValue ;
                                 owl:onProperty     anzsmls:physicalProperty
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  anzsmls:Field_pH ;
                                 owl:onProperty     anzsmls:pH
                               ] ;
        rdfs:subClassOf        [ a                  owl:Restriction ;
                                 owl:allValuesFrom  skos:Concept ;
                                 owl:onProperty     anzsmls:SoilFeature.observationMethod
                               ] ;
        iso19150-2:isAbstract  true ;
        skos:definition        "The abstract SoilFeature class represents a conceptual feature that is hypothesized to exist coherently in the world.\r\n   * this corresponds with a \"Soil Unit\", comprising a set of  similar soil profiles with a mappable distribution in some \"Soil Landscape\", a Soil Profile or its constituent parts;\r\n\r\nThe implemented SoilFeature instance acts as the \"description package\"\r\n\r\n    * the description package is classified according to its purpose as an Instance, TypicalNorm, or DefiningNorm."@en .

anzsmls:sourceRole  a    owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "The role of the source soil feature in the relationship. Examples might be 'soil complex'"@en .

anzsmls:purpose  a       owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "Specification of the intended purpose/level of abstraction for a given feature or object instance. Values are: \r\ninstance, typicalNorm, definingNorm."@en .

anzsmls:massStrength  a  owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "Broad strength classes based on the consolidation level (p215)\r\nExamples are 'Earth or soil', 'very weak rock', 'weak rock', 'moderately strong rock', 'strong rock'"@en .

anzsmls:Roots.size  a    owl:ObjectProperty ;
        rdfs:domain      anzsmls:Roots ;
        rdfs:range       swe:QuantityRange ;
        skos:definition  "The size range of the roots"@en .

anzsmls:Crack  a         owl:Class ;
        rdfs:subClassOf  anzsmls:Void ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:crackWidth
                         ] ;
        skos:definition  "Planar voids"@en .

anzsmls:stickiness  a    owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "Stickiness is determined on wet soil by pressing the sample and observing adherence of soil to fingers.\r\nValues are:\r\nnon-sticky, slightly sticky, moderately sticky, very sticky"@en .

anzsmls:spacing  a       owl:ObjectProperty ;
        rdfs:range       swe:QuantityRange ;
        skos:definition  "The spacing of discontinuities or joints."@en .

anzsmls:Consistence.strength
        a                owl:ObjectProperty ;
        rdfs:domain      anzsmls:Consistence ;
        rdfs:range       skos:Concept ;
        skos:definition  "Strength of the soil is the resistance to breaking or deformation.\r\nValues are:\r\nloose, very weak, weak, firm, very firm, strong, very strong, rigid"@en .

anzsmls:waterRepellence
        a                owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "Examples: non water repellent, water repellent, strongly water repellent\r\nASLS Field Handbook (p191)"@en .

anzsmls:geologicDescription
        a           owl:ObjectProperty ;
        rdfs:range  skos:Concept .

anzsmls:SoilSurface  a   owl:Class ;
        rdfs:subClassOf  anzsmls:SoilFeature ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:soilDisturbance
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  anzsmls:MicroRelief ;
                           owl:onProperty     anzsmls:surfaceRelief
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:surfaceCondition
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:soilDisturbance
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  anzsmls:MicroRelief ;
                           owl:onProperty     anzsmls:surfaceRelief
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:surfaceCondition
                         ] ;
        skos:definition  "The Soil as it appears at the surface.\r\nThis can be considered as analogous to a Soil Layer with a depth of 0."@en .

anzsmls:relatedSoilFeature
        a           owl:ObjectProperty ;
        rdfs:range  anzsmls:SoilFeature .

anzsmls:fabric  a   owl:ObjectProperty ;
        rdfs:range  skos:Concept .

anzsmls:confidence  a    owl:DatatypeProperty ;
        rdfs:range       xsd:string ;
        skos:definition  "A text description providing the confidence of the classification assertion."@en .

anzsmls:Segregations.strength
        a                owl:ObjectProperty ;
        rdfs:domain      anzsmls:Segregations ;
        rdfs:range       skos:Concept ;
        skos:definition  "The strength of the segregations (ASLS Field Handbook p 198).\r\nExamples: weak, strong"@en .

anzsmls:PhysicalPropertyValue.depth
        a                owl:ObjectProperty ;
        rdfs:domain      anzsmls:PhysicalPropertyValue ;
        rdfs:range       swe:QuantityRange ;
        skos:definition  "The depth below the soil surface the measurement was made."@en .

anzsmls:MicroRelief.proportion
        a                owl:ObjectProperty ;
        rdfs:domain      anzsmls:MicroRelief ;
        rdfs:range       skos:Concept ;
        skos:definition  "<b>proportion </b>is the proportion of the microrelief of each component. May only apply to gilgai?"@en .

anzsmls:Cutans.type  a   owl:ObjectProperty ;
        rdfs:domain      anzsmls:Cutans ;
        rdfs:range       skos:Concept ;
        skos:definition  "The type of cutan.\r\nExamples: none, unspecified, clay skins, mangans, stress cutans, slickensides, other cutans"@en .

anzsmls:distinctness  a  owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "The ease and certainty with which a cutan is identified. It relates to thickness and colour contrast with adjacent material.\r\nExamples: faint, distinct, prominent"@en .

anzsmls:surfaceCondition
        a                owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "Some soils have characteristic appearance when dry. More than one term may be applied to the surface. (p189)\r\nExamples: cracking, self-mulching, loose, soft, firm, ..."@en .

anzsmls:Peds  a          owl:Class ;
        rdfs:subClassOf  anzsmls:SoilMaterial ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:Peds.type
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:grade
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:Peds.type
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:grade
                         ] ;
        skos:definition  "Peds is a specialization of SoilMaterial to allow describing individual natural soil aggregates consisting of a cluster of primary particles (peds).  Peds are separated from adjoining peds by surfaces of weakness that are recognizable as natural voids or by the occurrence of cutans (ASLS Field Handbook p 171).\r\n\r\nProperties different from those of SoilMaterials include the ped type (platy, prismatic, columnar, angular blocky), and the grade (degree of development and distinctness of peds, e.g. single grain, massive, weak, moderate, strong).\r\n\r\nInherited property particleGeometry:particleType is used to specify that the particle is an artificial fragment of type 'clod' or 'fragment' and its ConstituentPart:role is ped.\r\nConstituentPart:relatedMaterial is used to identify parting between peds."@en .

anzsmls:reference  a     owl:ObjectProperty ;
        rdfs:range       meta:Citation ;
        skos:definition  "The reference to the source of the classification scheme"@en .

anzsmls:Segregations.type
        a                owl:ObjectProperty ;
        rdfs:domain      anzsmls:Segregations ;
        rdfs:range       skos:Concept ;
        skos:definition  "The nature of the segregations.\r\nExamples: unidentified, calcareous, gypseous, manganiferous, ..."@en .

anzsmls:associatedSubstrate
        a           owl:ObjectProperty ;
        rdfs:range  anzsmls:Substrate .

anzsmls:horizonRDepth
        a                owl:ObjectProperty ;
        rdfs:range       swe:Quantity ;
        skos:definition  "The depth to the R Horizon or strongly cemented pan"@en .

anzsmls:soilThematicResult
        a                owl:ObjectProperty ;
        rdfs:range       swe:Quantity ;
        skos:definition  "-- Name --\r\nsoil thematic result\r\n-- Definition --\r\ncollection of numeric values of the soil for the related soil thematic property within the soil thematic object."@en .

anzsmls:Segregations  a  owl:Class ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:magnetism
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:Segregations.type
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:Segregations.abundance
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:Segregations.abundance
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:Segregations.strength
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:form
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:Segregations.size
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:form
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:Segregations.strength
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:Segregations.size
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:Segregations.type
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:magnetism
                         ] ;
        skos:definition  "discrete segregations that have accumulated in the soil because of the concentration of some constituent, usually by chemical or biological action (ASLS Field Handbook p195)"@en .

anzsmls:Segregations.abundance
        a                owl:ObjectProperty ;
        rdfs:domain      anzsmls:Segregations ;
        rdfs:range       swe:QuantityRange ;
        skos:definition  "The % abundance of segregations (p196)\r\n0, 0-2%, 2-10%, 10-20%,"@en .

anzsmls:rootComponent
        a           owl:ObjectProperty ;
        rdfs:range  anzsmls:Roots .

anzsmls:boundaries  a    owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "How distinct are the mottle boundaries (eg sharp, clear, diffuse))"@en .

anzsmls:Roots  a         owl:Class ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:Roots.size
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:Roots.abundance
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:Roots.size
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:Roots.abundance
                         ] ;
        skos:definition  "The presence and description of plant roots within soil layers (p199)"@en .

anzsmls:PhysicalPropertyValue.observationMethod
        a                owl:ObjectProperty ;
        rdfs:domain      anzsmls:PhysicalPropertyValue ;
        rdfs:range       skos:Concept ;
        skos:definition  "The method used to obtain the quantitative value. QuantitativeObservationMethodTerm indicates that it should come from a vocabulary."@en .

anzsmls:Field_pH  a      owl:Class ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:double ;
                           owl:onProperty     anzsmls:pHValue
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     anzsmls:pHMethod
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:Quantity ;
                           owl:onProperty     anzsmls:pHDepth
                         ] ;
        skos:definition  "The soil pH determined in the field."@en .

anzsmls:physicalProperty
        a           owl:ObjectProperty ;
        rdfs:range  anzsmls:PhysicalPropertyValue .

anzsmls:magnetism  a     owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "The magnetic attributes of the segregations (ASLS Field Handbook p 198).\r\nExamples: non-magnetic, magnetic"@en .

anzsmls:moistureStatus
        a                owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "the moisture status of the soil. Maybe 'moist' or 'dry'.\r\nASLS Field Handbook p159"@en .

anzsmls:Roots.abundance
        a                owl:ObjectProperty ;
        rdfs:domain      anzsmls:Roots ;
        rdfs:range       skos:Concept ;
        skos:definition  "The abundance of the roots (p199)\r\nExamples: none, few, common, many, abundant"@en .

anzsmls:boundaryShape
        a                owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "The boundary shape between soil features.\r\nExamples for SoilLayers (ASLS p200) are: smooth, wavy, irregular, tongued, broken"@en .

anzsmls:contrast  a      owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "How distinct the mottles are (eg faint, distinct, prominent)"@en .

anzsmls:form  a          owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "The form of the segregations (ASLS Field Handbook p 196).\r\nExamples: concretions, nodules, fragments,..."@en .

anzsmls:uncertainty  a   owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "-- Name --\r\nsoil thematic property\r\n-- Definition --\r\nthe soil related property that the soil thematic results relate to within the soil thematic object.\r\nData Type is Any. However, the soilThematicProperty shall be a phenomenon associated with soil features."@en .

anzsmls:depthInterval
        a                owl:ObjectProperty ;
        rdfs:range       swe:QuantityRange ;
        skos:definition  "-- Name --\r\ndepth interval\r\n\r\n-- Definition --\r\nrange of depth from the surface to which the Soil Thematic Object is referring to.\r\n\r\n-- Description --\r\nA Soil Thematic Object can refer to a soil property for a particular depth range. For example, the texture class from 0 to 30 cm. Thus the DepthRange value will refer to the 0-30 cm depth range."@en .

anzsmls:SoilProfileAssociation
        a                owl:Class ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:SoilProfileAssociation.profileRole
                         ] ;
        skos:definition  "Association to allow specifying the role the SoilProfile is playing in the Soil Unit."@en .

anzsmls:Mottle.type  a   owl:ObjectProperty ;
        rdfs:domain      anzsmls:Mottle ;
        rdfs:range       skos:Concept ;
        skos:definition  "type of mottle (eg mottle, biological mixing, mechanical mixing, weathering)"@en .

anzsmls:SoilThematicObject
        a                owl:Class ;
        rdfs:subClassOf  anzsmls:AbstractSoil ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:depthInterval
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:Quantity ;
                           owl:onProperty     anzsmls:soilThematicResult
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:uncertainty
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:Quantity ;
                           owl:onProperty     anzsmls:soilThematicResult
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:soilThematicProperty
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:uncertainty
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  skos:Concept ;
                           owl:onProperty     anzsmls:soilThematicProperty
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:depthInterval
                         ] ;
        skos:definition  "-- Name --\r\nsoil thematic object\r\n\r\n-- Definition --\r\nspatial object representing a soil related property derived from one or more soil properties.\r\n\r\n-- Description --\r\nNOTE Soil thematic maps can be derived directly from the involved soil database (organic matter content, pH, texture, etc.) or they can be derived by using Pedotransfer Functions (water availability…)."@en .

anzsmls:SoilProfileAssociation.profileRole
        a                owl:ObjectProperty ;
        rdfs:domain      anzsmls:SoilProfileAssociation ;
        rdfs:range       skos:Concept ;
        skos:definition  "Describes whether the SoilProfile is typical, defining, theoretical"@en .

anzsmls:Void  a          owl:Class ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:macroporeDiameter
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  swe:QuantityRange ;
                           owl:onProperty     anzsmls:macroporeAbundance
                         ] ;
