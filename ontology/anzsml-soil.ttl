anzsmls:SoilPan.type  a  owl:ObjectProperty ;
        rdfs:domain      anzsmls:SoilPan ;
        rdfs:range       skos:Concept ;
        skos:definition  "Type of pan (ASLS Field Handbook p 192).\r\nExamples are: no pan, calcrete, silcrete, red-brown hardpan, duripan, fragipan, ..."@en .

anzsmls:cementation  a   owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "Degree of cementation;\r\nValues: uncemented, weakly cemented, moderately cemented, strongly cemented, very strongly cemented"@en .

anzsmls:propertyName  a  owl:ObjectProperty ;
