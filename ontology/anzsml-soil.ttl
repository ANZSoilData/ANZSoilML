anzsmls:pHDepth  a       owl:ObjectProperty ;
        rdfs:range       swe:Quantity ;
        skos:definition  "Depth at which the field pH value was obtained"@en .

anzsmls:landformPosition
        a                owl:ObjectProperty ;
        rdfs:range       skos:Concept ;
        skos:definition  "A soil site may not be centrally located in the associated LandformElement. The landformPosition property allows specifying where in the landform element the Soil Feature is located, relative to the height of the landform. \r\nValues are: top-third, middle-third, bottom-third"@en .

anzsmls:soilColour  a  owl:ObjectProperty ;
